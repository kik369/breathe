{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "functions",
	"main": "src/index.ts",
	"compatibility_date": "2025-08-23",
	"compatibility_flags": [
		"global_fetch_strictly_public"
	],
	"assets": {
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
    "route": {
        "pattern": "functions.kris2511.workers.dev/*",
        "zone_name": "kris2511.workers.dev"
    }
}